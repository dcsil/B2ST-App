{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon",
    "test": "jest --coverage --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^7.17.4",
    "@sentry/tracing": "^7.17.4",
    "bcrypt": "^5.1.0",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "mockingoose": "^2.15.2",
    "mongodb": "^4.11.0",
    "mongoose": "^6.7.1",
    "package.json": "^2.0.1",
    "request": "^2.88.2",
    "stripe": "^11.1.0",
    "supertest": "^6.3.1",
    "twilio": "^3.83.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "jest": "^29.3.0",
    "nyc": "^15.1.0"
  },
  "jest": {
    "testTimeout": 30000,
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>/routes"
    ],
    "moduleDirectories": [
      "node_modules",
      "routes"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "collectCoverageFrom": [
      "**/routes/**/*.js",
      "!**/node_modules/**",
      "**/config/*.js",
      "**/marketing_system/**/*.js",
      "**/models/*.js"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": ".."
        }
      ],
      [
        "text",
        {
          "skipFull": true
        }
      ],
      "text-summary"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
